{
    "cfgname" : "Visual studio",
    "author" : [ "DeinsOne <deinsone@gmail.com>" ],
    "edition" : "2021.10.06",
    "version" : "0.1.0",

    "cfg" : {
        "window" : {
            "title" : [
                "visual studio",
                "code"
            ],
            "method" : "WITH_KEYWORDS"
        },

        "interval" : 60

    },

    "targets" : {
        "branchMaster" :     { "text": "master" },
        "branchDev" :        { "text": "dev" }
    },

    "rules" : {
        "ShowBranchesFromMaster" : {
            "target": [ "branchMaster" ],
            "do": "Envi::SendInput(Envi::MouseButtonEvent{true, Envi::MouseButtons::LEFT}, std::chrono::microseconds(1))"
        },

        "ShowBranchesFromDev" : {
            "target": "branchDev",
            "do": "Envi::SendInput(Envi::MouseButtonEvent{true, Envi::MouseButtons::LEFT}, std::chrono::microseconds(1))"
        }

    }
}
