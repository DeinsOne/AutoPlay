{
    "title" : "Visual studio",
    "author" : [ "DeinsOne <deinsone@gmail.com>" ],
    "edition" : "2021.10.06",
    "version" : "0.1.0",

    "adapter" : {
        "type": "desktop",
        "window" : [
            "visual studio",
            "code"
        ],
        "method" : "with_keywords"
    },

    "interval" : 60,

    "targets" : {
        "branchMaster" :     { "text": "master" },
        "branchDev" :        { "text": "dev" }
    },

    "rules" : {
        "ShowBranchesFromMaster" : {
            "target": [ "branchMaster" ],
            "execute": "Envi::SendInput(Envi::MouseButtonEvent{true, Envi::MouseButtons::LEFT}, std::chrono::microseconds(1))"
        },

        "ShowBranchesFromDev" : {
            "target": [ "branchDev" ],
            "execute": "Envi::SendInput(Envi::MouseButtonEvent{true, Envi::MouseButtons::LEFT}, std::chrono::microseconds(1))"
        }
    }
}
